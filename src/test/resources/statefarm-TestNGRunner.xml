<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="start smoke testing">

    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="11"></parameter>
    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="browserVersion" value="99"></parameter>
    <parameter name="url" value="https://www.statefarm.com/"></parameter>

        <test name="logInTest">
            <classes>
                <class name="reporting.statefarm.account.Login"></class>
            </classes>
        </test>
        <test name="PersonalBankingTest">
            <classes>
                <class name="reporting.statefarm.banking.PersonalBanking"></class>
            </classes>
        </test>
        <test name="BusinessBankingTest">
            <classes>
               <class name="reporting.statefarm.banking.BusinessBanking"></class>
            </classes>
        </test>
        <test name="ClaimsTest">
            <classes>
                <class name="reporting.statefarm.claims.TypesOfClaims"></class>
            </classes>
        </test>
        <test name="AccessTest">
            <classes>
                <class name="reporting.statefarm.DocumentsAndMessages.Access"></class>
            </classes>
        </test>
        <test name="HelpTest">
            <classes>
                <class name="reporting.statefarm.help.HelpAndFix"></class>
            </classes>
        </test>
        <test name="AutoInsuranceTest">
            <classes>
               <class name="reporting.statefarm.insurance.AutoInsurance"></class>
            </classes>
        </test>
        <test name="SmallBusinessInsuranceTest">
            <classes>
               <class name="reporting.statefarm.insurance.SmallBusinessInsurance"></class>
            </classes>
        </test>
        <test name="HomeInsuranceTest">
            <classes>
                <class name="reporting.statefarm.insurance.HomeInsurance"></class>
            </classes>
        </test>
        <test name="LifeInsuranceTest">
            <classes>
                <class name="reporting.statefarm.insurance.LifeInsurance"></class>
            </classes>
        </test>
        <test name="RetirementInvestmentTest">
            <classes>
                <class name="reporting.statefarm.investment.Retirement"></class>
            </classes>
        </test>
        <test name="SavingForCollegeTest">
            <classes>
                  <class name="reporting.statefarm.investment.EducationSaving"></class>
            </classes>
        </test>
        <test name="MutualFunds">
            <classes>
                 <class name="reporting.statefarm.investment.MutualFund"></class>
            </classes>
        </test>
        <test name="LearnMoreTest">
            <classes>
                  <class name="reporting.statefarm.investment.RealEstatePlaning"></class>
            </classes>
        </test>
        <test name="BillingTest">
            <classes>
                  <class name="reporting.statefarm.payement.Billing"></class>
            </classes>
        </test>
        <test name="QuoteTest">
            <classes>
                 <class name="reporting.statefarm.quotes.DifferentQuotes"></class>
            </classes>
        </test>
        <test name="SearchTest">
            <classes>
                 <class name="reporting.statefarm.search.DifferentSearch"></class>
           </classes>
        </test>
        <test name="OtherFunctions">
            <classes>
                 <class name="reporting.statefarm.otherFunctions.Others"></class>
            </classes>
        </test>
    </suite>